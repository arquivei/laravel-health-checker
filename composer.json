{
  "name": "arquivei/laravel-health-checker",
  "description": "Laravel Health Checker",
  "license": "Apache-2.0",
  "keywords": [
    "health",
    "health-checker",
    "laravel"
  ],
  "require": {
    "php": "^7.3",
    "laravel/framework": "^5.7 || ^7.15"
  },
  "require-dev": {
    "predis/predis": "^1.0",
    "mockery/mockery" : "^1.3.3",
    "orchestra/testbench": "^3.7.0"
  },
  "suggest": {
    "guzzlehttp/guzzle": "~6.0",
    "predis/predis": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Laravel\\Health\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Laravel\\Health\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\Health\\Providers\\HealthServiceProvider"
      ],
      "aliases": {
        "HealthChecker": "Laravel\\Health\\Facades\\HealthFacade"
      }
    }
  },
  "scripts": {
    "test": [
      "@composer install",
      "vendor/bin/phpunit"
    ]
  }
}
